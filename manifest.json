{
    "manifest_version": 3,
    "permissions": [
      "storage"
    ],
    "content_scripts": [
      {
        "matches": [
          "*://*/*"
        ],
        "js": [
          "neuralyzer.js",
          "disable-links.js"
        ],
        "css": [
          "neuralyzer.css"
        ],
        "all_frames": true
      }
    ],
    "action": {
      "default_popup": "index.html"
    }
  }